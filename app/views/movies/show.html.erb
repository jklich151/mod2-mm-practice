<%= @movie.name %>
<%= @movie.creation_year %>
<%= @movie.genre %>
<% @movie.actors_by_age.each do |actor| %>
<%= actor.name %>
<% end %>
<%= @movie.actors_average_age %>

<%= form_tag "/movie_actor/#{@movie.id}" do %>
